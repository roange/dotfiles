# independent keys
super + z
	st

super + shift + z
    	kitty

super + x
	sublime_text

super + shift + x 
	kcolorchooser

super + v
	caja

super + l
	pavucontrol

super + shift + l 
	lxappearance

super + d
	dmenu_run

super + 0
	pamixer -i 4

super + 9
	pamixer -d 4


# scripts

super + s
	~/scripts/magick-import.sh


#xdotool + wmctrl

super + c
	wmctrl -ic $(xdotool getactivewindow)

super + e
	xdotool getactivewindow windowminimize

super + a
	wmctrl -ir $(xdotool getactivewindow) -b toggle,fullscreen

super + Escape 
	pkill -USR1 -x sxhkd

control + 1
	xdotool set_desktop 0

control + 2
	xdotool set_desktop 1

control + 3
	xdotool set_desktop 2

control + 4
	xdotool set_desktop 3

control + 5 
	xdotool set_desktop 4

	

